{### bewertungsart ################################################################}
CREATE TABLE bewertungsart (
   id        		SERIAL NOT NULL,							{ Primary Key       		}
   key        	VARCHAR(10),	  							{ Objekt-Tabelle				}
   dtxt  				VARCHAR(500), 								{ Information						}
   sort        	VARCHAR(5),	  								{ Sortierung						}
   zeitstempel  DATETIME year to fraction(5)	{ Aenderungsdatum-/zeit	}
);
